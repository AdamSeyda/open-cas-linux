SCRIPTPATH=`dirname $0`
SCRIPTPATH=`realpath $SCRIPTPATH`

CONFIG_FILES=`ls $SCRIPTPATH/configure.d/*.conf`

rm -f $SCRIPTPATH/modules/generated_defines.h

for file in $CONFIG_FILES; do
	/bin/bash $file
done

